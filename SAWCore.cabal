Name: SAWCore
Description: Core SAW terms
Version: 0.1
Author: Galois, Inc.
Maintainer: jhendrix@galois.com
Build-type: Simple
cabal-version: >= 1.8

library
  build-depends:
    array,
    base >= 4,
    bytestring,
    containers,
    directory,
    filepath,
    mtl,
    utf8-string
  hs-source-dirs: src
  exposed-modules:
     Verifier.SAW.Lexer
     Verifier.SAW.Parser
  other-modules:
     Verifier.SAW.AST
     Verifier.SAW.Position
     Verifier.SAW.TypedAST
  GHC-options: -Wall

